pipeline {
    agent any
    stages {
        stage('BuildStep') {
            steps {
                bat label: '', script: '''mvn clean install -DskipTests=false'''
            }
        }
        stage('Test') { 
            steps {
                bat label: '', script: '''mvn test'''
                echo 'hello testing done'
            }
        }
        stage('Packaging'){
            steps{
                bat label: '', script: '''mvn package'''
            }
        }
    }
}
